{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ES2015", "DOM"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "strict": true,    
    "module": "es2020",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}